/* backpageF/pages/ding_xiang/ding_xiang.wxss */
.dingXinag{
  padding: 10px 0;
  background: #eee;
  height: 96%;
  width: 100%;
}
page{
  height: 100%;
}
.dingXinag_top{
  padding: 0 10px;
  display: flex;
  height: 40px;
  align-items: center;
  color: red;
  background: #fff;
}
.dingXinag_kuai{
  background: #fff;
  padding: 10px 10px;
  border-bottom: 1px solid #eee;
  border-top: 1px solid #eee;
}
.dingXinag_top>image{
  width: 20px;
  height: 20px;
  margin-right: 10px;
}
.dingXinag_di{
  border: none;
}
.dingXiang_kuai_one{
  height: 20px;
  font-size: 10px;
  display: flex;
  align-content: center;
}
.dingXiang_kuai_one>image{
  width: 10px;
  height: 10px;
  color: #ccc;
  margin-right: 10px;
}
.dingXiang_kuai_two{
  margin-left: 20px;
  color: #ccc;
  font-size: 10px;
}
.dingXinag_list{
  background: #fff;
  margin: 10px 0;
  padding: 0 10px;
}
.dingXinag_list>view:first-child{
  height: 40px;
  line-height: 40px;
  font-size: 14px;
  border-bottom: 1px solid #eee;
}
.dingXinag_list_middle{
  display: flex;
  padding: 10px 0;
}
.dingXinag_list_middle>image{
  width: 70px;
  height: 70px;
}
.dingXinag_list_middle_two{
  flex: 1;
  display: flex;
  flex-direction: column;
  font-size: 10px;
  line-height: 35px;
  padding-left: 10px;
}
.dingXinag_list_middle_two>view:nth-child(2){
  color: #ccc;
}
.dingXinag_list_middle_three{
 display: flex;
  flex-direction: column;
  font-size: 10px;
  line-height: 35px;
}
.dingXinag_list_middle_three>view:nth-child(2){
  color: #ccc;
}
.dingXiang_bottom{
  background: #fff;
}
.dingXiang_bottom_one{
  display: flex;
  justify-content: space-between;
  height: 40px;
  align-items: center;
}